#Container{
	margin-top: 10px;
	margin-bottom: 10px;
}

#ButtonsBox{
	display: flex;
	justify-content: center;
	align-items: center;
}

#button{
	width: 50px;
	height: 20px;
	position: relative;
	margin-right: 2px;
	margin-bottom: 2px;
	background-color: #FFD12F;
}

#Label{
	color: #3A3A3A;
}

#Buttons{
	max-width: 100%;
	display: flex;
	flex-direction: row;
	justify-content: flex-end;
}

#Row{
	display: flex;
	flex-direction: row;
}